<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="100dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/ryparent"
    android:background="@color/blue"
    >

    <TextView
        android:id="@+id/tv_member"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="회원"
        app:layout_constraintBottom_toTopOf="@+id/tv_member_number"
        android:layout_marginStart="30dp"
        android:textColor="@color/white"
        app:layout_constraintVertical_chainStyle="packed"
        />

    <TextView
        android:id="@+id/tv_member_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_member"
        app:layout_constraintStart_toStartOf="@+id/tv_member"
        android:text="회원번호"
        android:textColor="@color/white"
        />


</androidx.constraintlayout.widget.ConstraintLayout>